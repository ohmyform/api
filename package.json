    "handlebars": "^4.7.6",
    "inquirer": "^7.1.0",
    "migrate-mongoose": "^4.0.0",
    "mongoose": "^5.9.11",
    "nestjs-console": "^3.0.2",
    "nestjs-pino": "^1.2.0",
    "nodemailer": "^6.4.6",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pino-pretty": "^4.0.0",
