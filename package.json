    "@nestjs/graphql": "^7.3.7",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/mongoose": "^6.4.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "apollo-server-express": "^2.13.0",
    "bcrypt": "^4.0.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "commander": "^5.1.0",
